<?xml version="1.0"?>
<launch>
  <arg name="rviz_config" default="$(find-pkg-share create_visualizer)/rviz/roomba.rviz" description="rviz config"/>

    <!-- Rviz -->
    <node
      pkg="rviz2"
      exec="rviz2"
      name="rviz2"
      output="screen"
      args="-d $(var rviz_config)"
    />
</launch>
